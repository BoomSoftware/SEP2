         <image>
            <Image url="@../../../Addition/Images/icons/uploadImage.png" />
         </image>
      </ImageView>
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="283.0" layoutY="14.0" prefHeight="43.0" prefWidth="491.0" text="ADVERTISEMENT TITLE" />
      <TextArea fx:id="descriptionTextArea" layoutX="283.0" layoutY="109.0" prefHeight="254.0" prefWidth="491.0" text="FDDFDFFDFDFDFDF;;;;;;;;;;;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;&#10;" />
      <Label layoutX="283.0" layoutY="86.0" text="Description" />
      <DatePicker fx:id="datePicker" layoutX="340.0" layoutY="381.0" prefHeight="26.0" prefWidth="70.0" />
