            <Image url="@../../../Addition/Images/icons/uploadImage.png" />
         </image>
         <viewport>
            <Rectangle2D height="300.0" width="300.0" />
         </viewport>
      </ImageView>
      <ImageView fx:id="sub1ImageView" fitHeight="90.0" fitWidth="90.0" layoutX="49.0" layoutY="250.0" onMouseClicked="#openSub1Picture" pickOnBounds="true" preserveRatio="true">
