<AnchorPane fx:id="mainPane" prefHeight="500.0" prefWidth="800.0" styleClass="white-pane" stylesheets="@../../../Addition/Styles/Styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ESharing.Client.Views.AdvertisementView.AdvertisementViewController">
