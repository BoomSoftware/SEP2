<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" styleClass="welcome-info-pane" stylesheets="@../../../Addition/Styles/Styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ESharing.Client.Views.MainAppView.MainAppViewController">
   <children>
      <Pane prefHeight="600.0" prefWidth="80.0" styleClass="navigate-pane">
         <children>
            <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="-1.0" layoutY="571.0" onMouseClicked="#onLogout" pickOnBounds="true" preserveRatio="true" rotate="-180.0" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/logout.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="29.0" layoutY="282.0" onMouseClicked="#onChatButton" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/messageIcon.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="29.0" layoutY="323.0" onMouseClicked="#onSettingButton" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/settingIcon.png" />
               </image>
            </ImageView>
            <Rectangle fx:id="messageRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="44.0" layoutY="273.0" stroke="BLACK" strokeType="INSIDE" styleClass="navigate-rectangle" width="4.0" />
            <Rectangle fx:id="settingRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="44.0" layoutY="313.0" stroke="BLACK" strokeType="INSIDE" styleClass="navigate-rectangle" width="4.0" />
            <Label fx:id="messageNotification" layoutX="45.0" layoutY="273.0" prefHeight="18.0" prefWidth="18.0" styleClass="message-notification" text="12" />
            <ImageView fitHeight="20.0" fitWidth="20.0" layoutX="21.0" layoutY="2.0" onMouseClicked="#onMinimizeAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Addition/Images/icons/minimize.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="20.0" fitWidth="20.0" layoutX="2.0" layoutY="2.0" onMouseClicked="#onCloseButtonAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Addition/Images/icons/close.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="40.0" fitWidth="40.0" layoutX="20.0" layoutY="52.0" onMouseClicked="#onGoToAddAdvertisement" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/addAdvertisement.png" />
               </image>
            </ImageView>
            <Rectangle fx:id="addAdvertisementRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="44.0" layoutY="50.0" stroke="BLACK" strokeType="INSIDE" styleClass="navigate-rectangle" width="4.0" />
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="29.0" layoutY="237.0" onMouseClicked="#onHomeAction" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/home.png" />
               </image>
            </ImageView>
            <Rectangle fx:id="homeRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="44.0" layoutY="228.0" stroke="BLACK" strokeType="INSIDE" styleClass="navigate-rectangle" width="4.0" />
         </children>
      </Pane>
      <Pane fx:id="contentPane" layoutX="80.0" prefHeight="600.0" prefWidth="820.0" styleClass="white-pane" />
   </children>
</AnchorPane>
