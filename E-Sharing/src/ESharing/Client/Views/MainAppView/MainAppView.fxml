<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" styleClass="welcome-info-pane" stylesheets="@../../../Addition/Styles/Styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ESharing.Client.Views.MainAppView.MainAppViewController">
   <children>
      <Pane prefHeight="500.0" prefWidth="50.0" styleClass="navigate-pane">
         <children>
            <ImageView fitHeight="30.0" fitWidth="38.0" layoutX="10.0" layoutY="456.0" onMouseClicked="#onLogout" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/logout.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="13.0" layoutY="278.0" onMouseClicked="#onChatButton" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/messageIcon.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="13.0" layoutY="320.0" onMouseClicked="#onSettingButton" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/settingIcon.png" />
               </image>
            </ImageView>
            <Rectangle fx:id="messageRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="44.0" layoutY="268.0" stroke="BLACK" strokeType="INSIDE" styleClass="navigate-rectangle" width="4.0" />
            <Rectangle fx:id="settingRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="44.0" layoutY="310.0" stroke="BLACK" strokeType="INSIDE" styleClass="navigate-rectangle" width="4.0" />
            <Label fx:id="messageNotification" layoutX="36.0" layoutY="270.0" prefHeight="17.0" prefWidth="12.0" styleClass="message-notification" text="12" />
            <ImageView fitHeight="18.0" fitWidth="18.0" layoutX="20.0" layoutY="2.0" onMouseClicked="#onMinimizeAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Addition/Images/icons/minimize.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="18.0" fitWidth="18.0" layoutX="2.0" layoutY="2.0" onMouseClicked="#onCloseButtonAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Addition/Images/icons/close.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="13.0" layoutY="59.0" onMouseClicked="#onGoToAddAdvertisement" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/addAdvertisement.png" />
               </image>
            </ImageView>
            <Rectangle fx:id="addAdvertisementRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="44.0" layoutY="50.0" stroke="BLACK" strokeType="INSIDE" styleClass="navigate-rectangle" width="4.0" />
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="13.0" layoutY="238.0" onMouseClicked="#onHomeAction" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../../../Addition/Images/icons/home.png" />
               </image>
            </ImageView>
            <Rectangle fx:id="homeRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="44.0" layoutY="228.0" stroke="BLACK" strokeType="INSIDE" styleClass="navigate-rectangle" width="4.0" />
         </children>
      </Pane>
      <Pane fx:id="contentPane" layoutX="50.0" prefHeight="500.0" prefWidth="750.0" styleClass="message-navigate-pane">
         <children>
            <ScrollPane fx:id="scrollPane" layoutY="47.0" prefHeight="453.0" prefWidth="750.0">
               <content>
                  <VBox fx:id="mainVBox" prefHeight="449.0" prefWidth="747.0" />
               </content>
            </ScrollPane>
            <JFXComboBox fx:id="searchComboBox" layoutX="570.0" layoutY="14.0" onAction="#onSearchAdvertisements" prefHeight="25.0" prefWidth="130.0" />
         </children></Pane>
   </children>
</AnchorPane>
